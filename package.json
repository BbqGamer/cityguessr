{
  "name": "cityguessr",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "clean": "rm -rf dist/*",
    "copy-assets": "cp -R src/views dist/src",
    "scss": "node-sass src/public/scss -o dist/src/public/css",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc copy-assets scss",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs,scss --exec npm run dev:start",
    "start": "node dist/src/app.js",
    "chroma": "chroma run --path data/chroma.db",
    "smtp": "python3 -m smtpd -c DebuggingServer -n localhost:1025",
    "all": "npm-run-all start smtp chroma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "node-sass": "^9.0.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "chromadb": "^1.7.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.8",
    "openai": "^4.24.4",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1"
  }
}
